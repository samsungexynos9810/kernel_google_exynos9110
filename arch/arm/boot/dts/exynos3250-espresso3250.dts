/dts-v1/;
#include "exynos3250.dtsi"


/ {
	model = "Samsung ESPRESSO3250 board based on EXYNOS3250";
	compatible = "samsung,ESPRESSO3250", "samsung,exynos3250";

	chosen {
		bootargs = "console=ttySAC2,115200 vmalloc=256M";
	};

	memory {
		reg = <0x40000000 0x20000000>;
	};

	fixed-rate-clocks {
		#address-cells = <1>;
		#size-cells = <0>;

		xrtcxti: clock@0 {
			compatible = "fixed-clock";
			reg = <0>;
			clock-frequency = <32768>;
			#clock-cells = <0>;
			clock-output-names = "xrtcxti";
		};

		xusbxti: clock@1 {
			compatible = "fixed-clock";
			clock-frequency = <24000000>;
			reg = <1>;
			#clock-cells = <0>;
			clock-output-names = "xusbxti";
		};

		xxti: clock@2 {
			compatible = "fixed-clock";
			reg = <2>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "xxti";
			};

		xtcxo: clock@3 {
			compatible = "fixed-clock";
			reg = <3>;
			clock-frequency = <26000000>;
			#clock-cells = <0>;
			clock-output-names = "xtcxo";
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		button@1 {
			label = "gpio-keys: KEY_POWER";
			linux,code = <116>;
			gpios = <&gpx2 7 0x1>;
			gpio-key,wakeup = <1>;
			debounce-interval = <0>;
		};

		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			linux,code = <114>;
			gpios = <&gpx1 7 0x1>;
			debounce-interval = <0>;
		};

		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			linux,code = <115>;
			gpios = <&gpx0 0 0x1>;
			debounce-interval = <0>;
		};

		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			linux,code = <172>;
			gpios = <&gpx0 3 0x1>;
			debounce-interval = <0>;
		};
	};

	i2c@13860000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;

		s2mps14_pmic@66 {
			compatible = "samsung,s2mps14-pmic";
			reg = <0x66>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpx0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			/* RTC: wtsr/smpl */
                        wtsr_en = <1>; /* enable */
                        smpl_en = <1>; /* enable */
                        wtsr_timer_val = <3>;   /* 1000ms */
                        smpl_timer_val = <4>;   /* 500ms */
                        check_jigon = <0>; /* do not check jigon */
                        /* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
                        init_time,sec = <0>;
                        init_time,min = <0>;
                        init_time,hour = <12>;
                        init_time,mday = <1>;
                        init_time,mon = <0>;
                        init_time,year = <114>;
                        init_time,wday = <3>;

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "vdd_mif";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "vdd_arm";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "vdd_int";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo15_reg: LDO15 {
					regulator-name = "vdd_tsp_3.3";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo16_reg: LDO16 {
					regulator-name = "vdd_lcd_3.3";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo19_reg: LDO19 {
					regulator-name = "vdd_tsp_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};
				ldo20_reg: LDO20 {
					regulator-name = "vdd_lcd_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};
			};
		};
	};

	dwmmc0@12510000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		fixed_volt;
		only_once_tune;
		extra_tuning;
		supports-highspeed;
		enable-cache-control;
		sw_data_timeout;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr200-timing = <1 1 2 0>;
		clk_pin = "gpk0-0";
		clk_addr = "11000000.pinctrl";
		clk_val = <0x3>;
		num-ref-clks = <8>;
		ciu_clkin = <50 50 50 100 100 100 100 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus1 &sd0_bus4 &sd0_bus8>;

		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

        ion {
                compatible = "samsung,exynos5430-ion";

		contig-names = "mfc_sh",
				"video",
				"mfc_fw",
				"mfc_nfw";

		contig = <2 0x100000 0x0>,
			<11 0xA00000 0x0>,
			<8 0x100000 0x0>,
			<12 0x100000 0x0>;

                ion_noncontig_heap {
                        id-type = <0 0>;
                };

		exynos_contig_heap {
			id-type = <4 6>;
		};

        };

	i2c@138B0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;
		status = "okay";

		touchscreen@48 {
			compatible = "melfas, mms134";
			reg = <0x48>;
			interrupts = <5 0 0>;
			interrupt-parent = <&gpx0>;
			max_x = <480>;
			max_y = <800>;
		};
	};

	i2c@13890000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		bq24160_charger@6b {
			compatible = "ti,bq24160";
			reg = <0x6b>;
			gpios = <&gpx2 1 1>;
			interrupts = <1 0 0>;
			interrupt-parent = <&gpx2>;
			pinctrl-names = "default";
			pinctrl-0 = <&chg_irq>;
			dev_name = "bq24160";
			support_boot_charging = <0>;
		};
	};

	sysreg_localout: sysreg_localout@10010000 {
		compatible = "samsung,exynos5-sysreg_localout";
		reg = <0x10010000 0x2000>;
	};

	mdev_0: mdev_output {
		compatible = "samsung,exynos5-mdev";
	};

	 fimd_fb: fimd_fb {
                /* gpx0[6]- TE external interrupt pin */
                gpios = <&gpx0 4 0xf>;
                fimd: fimd_ctrl {
                        samsung,left_margin = <1>;
                        samsung,right_margin = <1>;
                        samsung,upper_margin = <1>;
                        samsung,lower_margin = <1>;
                        samsung,hsync_len = <1>;
                        samsung,vsync_len = <1>;
                        samsung,xres = <480>;
                        samsung,yres = <800>;
                        samsung,virtual_x = <480>;
                        samsung,virtual_y = <800>;

                        samsung,width = <56>;
                        samsung,height = <94>;
                        samsung,max_bpp = <32>;
                        samsung,default_bpp = <24>;

                        fb_driver_data {
                                fb_win_variant_0 {
                                        has_osd_c = <1>;
                                        has_osd_d = <0>;
                                        has_osd_alpha = <0>;
                                        osd_size_off = <0x08>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_1 {
                                        has_osd_c = <1>;
                                        has_osd_d = <1>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0c>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_2 {
                                        has_osd_c = <1>;
                                        has_osd_d = <1>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0c>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_3 {
                                        has_osd_c = <1>;
                                        has_osd_d = <0>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_4 {
                                        has_osd_c = <1>;
                                        has_osd_d = <0>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                        };
                };

                mipi_dsi: mipi_dsi {
                        /* gpc0[0]-DISP_SEL(power1), gpc0[1]-Reset, gpc0[4]-LCD_BL_ON(power2) */
                        gpios = <&gpc0 0 0x1>, <&gpc0 1 0x1>, <&gpc0 4 0x1>;
                        interrupt-parent = <&gic>;
                        interrupts = <0 83 0>;

                        /* Interface definition: 0 - DSIM_COMMAND,
                                                 1 - DSIM_VIDEO */
                        e_interface = <0>;

                        /* Pixel format : 0 - DSIM_CMD_3BPP,
                                          1 - DSIM_CMD_8BPP,
                                          2 - DSIM_CMD_12BPP,
                                          3 - DSIM_CMD_16BPP,
                                          4 - DSIM_VID_16BPP_565,
                                          5 - DSIM_VID_18BPP_666PACKED,
                                          6 - DSIM_18BPP_666LOOSELYPACKED,
                                          7 - DSIM_24BPP_888 */
                        e_pixel_format = <7>;

                        /* main frame fifo auto flush at VSYNC pulse */
                        auto_flush = <0>;
                        eot_disable = <0>;
                        auto_vertical_cnt = <0>;
                        hse = <0>;
                        hfp = <0>;
                        hbp = <0>;
                        hsa = <0>;

                        /* num of data lane : 0 - DSIM_DATA_LANE_1,
                                              1 - DSIM_DATA_LANE_2,
                                              2 - DSIM_DATA_LANE_3,
                                              3 - DSIM_DATA_LANE_4 */
                        e_no_data_lane = <1>;

                        /* byte clock divider : 0 - DSIM_PLL_OUT_DIV8,
                                                1 - DSIM_EXT_CLK_DIV8,
                                                2 - DSIM_EXT_CLK_BYPASS */
                        e_byte_clk = <0>;

                        /* Burst mode : 0 - DSIM_NON_BURST_SYNC_EVENT,
                                        2 - DSIM_NON_BURST_SYNC_PULSE,
                                        1 - DSIM_BURST
                                        4 - DSIM_NON_VIDEO_MODE */
                        e_burst_mode = <1>;

                        /* PMS
                           - 1. LCD_S6E8AA0 : p - 4, m - 80, s - 2
                           - 2. LCD_S6E3FA0(COMMAND): p - 2, m - 46, s - 1
                           - 3. LCD_S6E3FA0(VIDEO): p - 4, m - 75, s - 1
                           - 4. LCD_S6E3HA0(COMMAND): p - 2, m - 46, s - 1
                           - 5. LCD_S6E63J0X03(CMD):  p - 3, m - 125, s -1 */
                        p = <2>;
                        m = <42>;
                        s = <0>;

                        /* D-PHY PLL stable time spec :min = 200usec ~ max 400usec */
                        pll_stable_time = <500>;
                        /* escape clk : 20MHz */
                        esc_clk = <20000000>;
                        /* stop state holding counter after bta change count 0 ~ 0xfff */
                        stop_holding_cnt = <0x0fff>;
                        /* bta timeout 0 ~ 0xff */
                        bta_timeout = <0xff>;
                        /* lp rx timeout 0 ~ 0xffff */
                        rx_timeout = <0xffff>;
                };
        };
};
