/dts-v1/;
/include/ "exynos3250-koi-pinctrl.dtsi"
#include "exynos3250.dtsi"
#include "exynos3250-koi-pd_gpio.dtsi"

/ {
	model = "Casio KOI board based on EXYNOS3250";
	compatible = "samsung,KOI", "samsung,SHIRI", "samsung,exynos3250";

	chosen {
		bootargs = "console=ttySAC2,115200 vmalloc=256M ess_setup=0x46000000";
	};

	memory {
		reg = <0x40000000 0x20000000>;
	};

	ramoops {
		compatible = "ramoops";
		reg = <0x41f00000 0x00100000>;
		record-size = <0x00020000>;
		dump-oops;
	};

	fixed-rate-clocks {
		#address-cells = <1>;
		#size-cells = <0>;

		xrtcxti: clock@0 {
			compatible = "fixed-clock";
			reg = <0>;
			clock-frequency = <32768>;
			#clock-cells = <0>;
			clock-output-names = "xrtcxti";
		};

		xusbxti: clock@1 {
			compatible = "fixed-clock";
			clock-frequency = <24000000>;
			reg = <1>;
			#clock-cells = <0>;
			clock-output-names = "xusbxti";
		};

		xxti: clock@2 {
			compatible = "fixed-clock";
			reg = <2>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "xxti";
			};

		xtcxo: clock@3 {
			compatible = "fixed-clock";
			reg = <3>;
			clock-frequency = <26000000>;
			#clock-cells = <0>;
			clock-output-names = "xtcxo";
		};
		audiocdclk: clock@4 {
			compatible = "fixed-clock";
			reg = <4>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "audiocdclk";
		};
	};

	bt_host_wake {
		compatible = "bt_host_wake";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&eint22>;
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&eint23 &non_eint3 &non_eint14 &eint0 &eint15>;

		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupt-parent = <&gpx2>;
			interrupts = <7 0 0>;
			linux,code = <116>;
			gpios = <&gpx2 7 0x1>;
			gpio-key,wakeup = <1>;
			debounce-interval = <0>;
		};

		button@2 {
			label = "gpio-keys: VOLUME_UP";
			interrupt-parent = <&gpx1>;
			interrupts = <7 0 0>;
			linux,code = <115>;
			gpios = <&gpx1 7 0x1>;
			gpio-key,wakeup = <1>;
			debounce-interval = <0>;
		};

		button@3 {
			label = "gpio-keys: VOLUME_DOWN";
			interrupt-parent = <&gpx0>;
			interrupts = <0 0 0>;
			linux,code = <114>;
			gpios = <&gpx0 0 0x1>;
			gpio-key,wakeup = <1>;
			debounce-interval = <0>;
		};
	};

	sec_pwm: pwm@139D0000 {
		status = "okay";
	};

	vibrator {
		compatible = "samsung,shiri-vib";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm1_out>;
		samsung,pwm-ch = <1>;
		samsung,max-timeout = <100000>;
	};

	i2c@13860000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;

		s2mps14_pmic@66 {
			compatible = "samsung,s2mps14-pmic";
			reg = <0x66>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpx0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
                        wtsr_en = <1>;
                        smpl_en = <1>;
                        wtsr_timer_val = <3>;
                        smpl_timer_val = <4>;
                        check_jigon = <0>;
                        /* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
                        init_time,sec = <0>;
                        init_time,min = <0>;
                        init_time,hour = <12>;
                        init_time,mday = <1>;
                        init_time,mon = <0>;
                        init_time,year = <114>;
                        init_time,wday = <3>;

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "vdd_mif";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "vdd_arm";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "vdd_int";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo2_reg: LDO2 {
					regulator-always-on;
					regulator-initial-mode = <1>;
                                };

				ldo3_reg: LDO3 {
                                        regulator-name = "LDO3";
					regulator-initial-mode = <3>;
					regulator-always-on;
                                };

				ldo4_reg: LDO4 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo5_reg: LDO5 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo6_reg: LDO6 {
                                        regulator-name = "VDD_USB_AP_1.0V";
                                        regulator-min-microvolt = <1000000>;
                                        regulator-max-microvolt = <1000000>;
					regulator-initial-mode = <1>;
                                };

				ldo7_reg: LDO7 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo8_reg: LDO8 {
                                        regulator-name = "VDD_UOTG_AP_3.3V";
                                        regulator-min-microvolt = <3000000>;
                                        regulator-max-microvolt = <3000000>;
					regulator-initial-mode = <1>;
                                };

				ldo15_reg: LDO15 {
					regulator-name = "vdd_tsp_3.1";
					regulator-min-microvolt = <3100000>;
					regulator-max-microvolt = <3100000>;
				};

				ldo16_reg: LDO16 {
					regulator-name = "vdd_lcd_3.1";
					regulator-min-microvolt = <3100000>;
					regulator-max-microvolt = <3100000>;
					regulator-initial-mode = <3>;
				};

				ldo17_reg: LDO17 {
					regulator-name = "vdd_aud_3.1";
					regulator-min-microvolt = <3100000>;
					regulator-max-microvolt = <3100000>;
					regulator-initial-mode = <1>;
				};

				ldo19_reg: LDO19 {
					regulator-name = "vdd_bt_dual_1v8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};

				ldo20_reg: LDO20 {
					regulator-name = "vdd_lcd_1.8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-initial-mode = <3>;
				};

				ldo21_reg: LDO21 {
					regulator-name = "vdd_tsp_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
				};

				ldo23_reg: LDO23 {
					regulator-name = "vdd_aud_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

			};
		};
	};

	i2c@13870000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <360000>;
		#size-cells = <0>;

		ak4678@12{
			compatible = "akm,ak4678";
			reg = <0x12>;
			ak4678,pdn-gpio = <&gpm0 4 0x1>;
			vdd_18-supply = <&ldo23_reg>;
			vdd_30-supply = <&ldo17_reg>;
		};
	};

	dwmmc0@12510000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		fixed_volt;
		clock-gate;
		only_once_tune;
		extra_tuning;
		supports-highspeed;
		enable-cache-control;
		sw_data_timeout;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr200-timing = <1 1 2 0>;
		num-ref-clks = <8>;
		gpios = <&gpk0 2 0x1>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus1 &sd0_bus4 &sd0_bus8>;

		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@12520000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		clock-gate;
		supports-highspeed;
		sw_data_timeout;
		use-broken-voltage;
		pm-ignore-notify;
		fixed_volt;
		cd-type = <2>;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		non-removable;
		supports-sdio-irq;
		enable-cclk-on-suspend;
		keep-power-in-suspend;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		clk_str_num = <4>;
		num-ref-clks = <3>;
		ciu_clkin = <25 50 50>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};

	};

	dwmmc2@12530000 {
		status = "okay";
		num-slots = <1>;
		clock-gate;
		bypass-smu;
		supports-highspeed;
		fixed_volt;
		cd-type = <1>;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

        ion {
                compatible = "samsung,exynos5430-ion";

                ion_noncontig_heap {
                        id-type = <0 0>;
                };

		exynos_contig_heap {
			id-type = <4 6>;
		};

        };

	i2c@13890000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;
		status = "okay";

		touchscreen@24 {
			compatible = "cy,cyttsp5_i2c_adapter";
			reg = <0x24>;
			interrupt-parent = <&gpx0>;
			interrupts = <5 0 0>;
			pinctrl-names = "default";
			pinctrl-0 = <&touch_irq &touch_rst>;
			cy,adapter_id = "cyttsp5_i2c_adapter";
			cy,core {
				cy,name = "cyttsp5_core";
				cy,irq_gpio = <&gpx0 5 0x1>;
				cy,rst_gpio = <&gpx2 5 0x1>;
				cy,hid_desc_register = <1>;
				cy,mt {
					cy,name = "cyttsp5_mt";
					cy,inp_dev_name = "cyttsp5_mt";
					cy,abs =
						/* ABS_MT_POSITION_X, CY_ABS_MIN_X, CY_ABS_MAX_X, 0, 0 */
						<0x35 0 320 0 0
						/* ABS_MT_POSITION_Y, CY_ABS_MIN_Y, CY_ABS_MAX_Y, 0, 0 */
						0x36 0 320 0 0
						/* ABS_MT_PRESSURE, CY_ABS_MIN_P, CY_ABS_MAX_P, 0, 0 */
						0x3a 0 255 0 0
						/* CY_IGNORE_VALUE, CY_ABS_MIN_W, CY_ABS_MAX_W, 0, 0 */
						0xffff 0 255 0 0
						/* ABS_MT_TRACKING_ID, CY_ABS_MIN_T, CY_ABS_MAX_T, 0, 0 */
						0x39 0 15 0 0
						/* ABS_MT_TOUCH_MAJOR, 0, 255, 0, 0 */
						0x30 0 255 0 0
						/* ABS_MT_TOUCH_MINOR, 0, 255, 0, 0 */
						0x31 0 255 0 0
						/* ABS_MT_ORIENTATION, -127, 127, 0, 0 */
						0x34 0xffffff81 127 0 0
						/* ABS_MT_TOOL_TYPE, 0, MT_TOOL_MAX, 0, 0 */
						0x37 0 1 0 0>;
						/* ABS_DISTANCE, 0, 255, 0, 0 */
						/*0x19 0 255 0 0>;*/
				};
			};
		};
	};

	i2c@138D0000 {
		samsung,i2c-sda-delay = <25>;
		samsung,i2c-max-bus-freq = <400000>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		modelgauge@36 {
			compatible = "maxim,max17058";

			reg = <0x36>;
			gpios = <&gpx2 2 1>;
			interrupt-parent = <&gpx2>;
			interrupts = <2 0 0>;
			pinctrl-names = "default";
			pinctrl-0 = <&gauge_irq>;

			maxim,empty_adjustment = /bits/ 8 <0>;
			maxim,full_adjustment = /bits/ 8 <100>;
			maxim,rcomp0 = /bits/ 8 <175>;
			maxim,temp-co-up = <(-1100)>;
			maxim,temp-co-down = <(-4000)>;
			maxim,ocvtest = /bits/ 16 <56144>;
			maxim,soc-check-a = /bits/ 8 <241>;
			maxim,soc-check-b = /bits/ 8 <243>;
			maxim,bits = /bits/ 8 <19>;

			maxim,model-data = /bits/ 8 <
				0x9B 0x70 0xAB 0x30 0xB5 0xA0 0xB9 0xD0
		0xBB 0xA0 0xBC 0x00 0xBC 0xB0 0xBD 0x00
				0xBD 0x60 0xBE 0x40 0xBF 0x40 0xC1 0xF0
				0xC5 0x60 0xC8 0xA0 0xCD 0x00 0xD1 0x50
				0x00 0xE0 0x01 0x80 0x18 0x60 0x1C 0x20
				0x54 0x00 0x6A 0xC0 0x79 0x20 0x65 0xC0
				0x0B 0xE0 0x2A 0xC0 0x1D 0x00 0x17 0xE0
				0x15 0xE0 0x11 0xE0 0x11 0x00 0x11 0x00	>;
		};

	};

	 fimd_fb: fimd_fb {
