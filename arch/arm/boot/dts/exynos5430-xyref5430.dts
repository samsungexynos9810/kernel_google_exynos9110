	i2c@14E10000 {
		gpios = <&gpj1 0 0 /* sda */
			 &gpj1 1 0 /* scl */
			>;
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;
		status = "okay";
		touchscreen@20 {
			compatible = "synaptics,rmi4";
			reg = <0x20>;
			gpios = <&gpj1 2 0xf>;
			sensor_max_x = <1079>;
			sensor_max_y = <1919>;
			max_touch_width = <28>;
			irq_type = <8200>;
			firmware_name = "tsp_synaptics/synaptics_b0_h.fw";
			fac_firmware_name = "tsp_synaptics/synaptics_b0_fac.fw";
			num_of_rx = <28>;
			num_of_tx = <16>;
			panel_revision = <8>;
		};


	};

	hsi2c_1: hsi2c@14E50000 {
		compatible = "samsung,exynos5-hsi2c";
		clock-frequency = <400000>;	/* 400KHz */
		status = "okay";
		audio_codec: codec@3e {
			compatible = "audience,es515";
			reg = <0x3E>;
			gpios = <&gpg3 4 0	/* reset */
				 &gpg3 5 0	/* wakeup */
				 &gpa0 5 0xF>;	/* interrupt */
		};
	};

	spi_0: spi@14d20000 {
		status = "disabled";
	};

	spi_1: spi@14d30000 {
		status = "disabled";
	};

	spi_2: spi@14d40000 {
		status = "disabled";
	};

