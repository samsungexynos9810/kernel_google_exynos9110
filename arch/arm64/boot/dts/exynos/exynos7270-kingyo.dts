/dts-v1/;
#include "exynos7270-kingyo.dtsi"

/ {
	model = "Exynos 7270 SMDK board";
	compatible = "samsung,exynos7570", "samsung,exynos4210";

	memory@40000000 {
		device_type = "memory";
		reg = <0x0 0x40000000 0x30000000>;
	};

	chosen {
		bootargs ="console=ttySAC2,115200 clk_ignore_unused androidboot.hardware=kingyo firmware_class.path=/vendor/firmware ess_setup=0x46000000";
		linux,initrd-start = <0x42000000>;
		linux,initrd-end = <0x420FFFFF>;
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos7570-oscclk";
			clock-frequency = <26000000>;
		};
	};

	/* Console */
	serial_2: uart@13820000 {
		status = "okay";
	};

	/* UART (SUBCPU) */
	usi_0: usi@101D2000 {
		status = "okay";
	};

	serial_3: uart@13910000 {
		status = "okay";
	};

	/* UART (BT) */
	usi_1: usi@101D3000 {
		status = "okay";
	};

	serial_4: uart@13930000 {
		status = "okay";
	};

	dwmmc0@13540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		fixed_voltage;
		supports-highspeed;
		mmc-ddr-1_8v;
		supports-8bit;
		supports-cmd23;
		supports-erase;
		supports-hs400-enhanced-strobe;
		enable-ulp-mode;
		use-fine-tuning;
		only_once_tune;
		bypass-for-allpass;
		card-init-hwacg-ctrl;
		fifo-depth = <0x40>;
		non-removable;
		desc-size = <4>;
		card-detect-delay = <200>;
		clock-frequency = <700000000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-txdt-crc-timer-fastlimit = <0x34>;
		samsung,dw-mshc-txdt-crc-timer-initval = <0x38>;
		samsung,dw-mshc-hs400-delay-line = <0x60>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-hs400-timing = <1 0 2 0>;
		samsung,dw-mshc-hs400-ulp-timing = <3 0 2 0>;

		num-ref-clks = <12>;
		ciu_clkin = <25 43 43 25 43 87 175 43 43 175 175 175>;

		/* Swapping clock drive strength */
		clk-drive-number = <4>;
		pinctrl-names = "default",
			"fast-slew-rate-1x",
			"fast-slew-rate-2x",
			"fast-slew-rate-3x",
			"fast-slew-rate-4x";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		pinctrl-1 = <&sd0_clk_fast_slew_rate_1x>;
		pinctrl-2 = <&sd0_clk_fast_slew_rate_2x>;
		pinctrl-3 = <&sd0_clk_fast_slew_rate_3x>;
		pinctrl-4 = <&sd0_clk_fast_slew_rate_4x>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc2@13560000 {
		status = "okay";
		num-slots = <1>;
		supports-4bit;
		supports-cmd23;
		supports-erase;
		clock-gate;
		fixed_volt;
		supports-highspeed;
//		only_once_tune;
		enable-cclk-on-suspend;
		keep-power-in-suspend;
//		enable-sdio-wakeup;
		skip-init-mmc-scan;
		non-removable;
		broken-card-detect;
		supports-poweroff-notification;
		card-detect-ext;
//		cd-gpio = <&gpc0 1 0xf>;
		qos_int_level = <133>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		num-ref-clks = <3>;
		ciu_clkin = <25 50 50>;

//		num-ref-clks = <11>;
//		ciu_clkin = <25 50 50 50 50 100 200 100 100 200 400>;
//		clk-drive-number = <4>;
		pinctrl-names = "default";
//			"fast-slew-rate-1x",
//			"fast-slew-rate-2x",
//			"fast-slew-rate-3x",
//			"fast-slew-rate-4x";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4>;
//		pinctrl-1 = <&sd2_clk_fast_slew_rate_1x>;
//		pinctrl-2 = <&sd2_clk_fast_slew_rate_2x>;
//		pinctrl-3 = <&sd2_clk_fast_slew_rate_3x>;
//		pinctrl-4 = <&sd2_clk_fast_slew_rate_4x>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
//			disable-wp;
		};
	};

	ion {
		compatible = "samsung,exynos5430-ion";
	};

	usb@13600000 {
		status = "okay";

		dwc3 {
			dr_mode = "otg";
		};
	};

	usbphy@135c0000 {
		status = "okay";
	};

	usb_switch {
		compatible = "samsung,exynos-usb-switch";
		udc = <&udc>;
		pinctrl-names = "default";
		pinctrl-0 = <&sub_main_vbus_det>;
		samsung,bsess-gpio = <&gpa1 1 0>;
	};

	bluetooth {
		compatible = "kingyo-bluetooth";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&dual_main_wkup &main_dual_rst>;
	};

	wlan {
		compatible = "kingyo-wifi";
		pinctrl-names = "default";
		pinctrl-0 = <&wlan_main_wkup &wlan_reset>;
	};

	kingyo_gps {
		compatible = "kingyo-gps";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&sub_main_int2>;
		sub-main-int2 = <&gpa1 3 0>;
	};

