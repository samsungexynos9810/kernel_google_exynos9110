	i2c@13880000 {
		status = "okay";
		muic-s2mm001@25 {
			compatible = "samsung,muic-s2mm001";
			reg = <0x25>;
			pinctrl-names = "default";
			muic,muic_int = <&gpa1 1 0>;
		};
	};
*/
	speedy@10510000 {
		status = "okay";
		s2mpw01mfd@00 {
			compatible = "samsung,s2mpw01mfd";
			reg = <0x00>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_main_irq>;
			s2mpw01,irq-gpio = <&gpa0 5 0>;
			s2mpw01,wakeup = "enabled";

			i2c-speedy-address;
			cache_data = <1>;

			/* RTC: wtsr/smpl */
			wtsr_en = "enabled"; /* enable */
			wtsr_timer_val = <3>;   /* 1000ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: reset time to 1/1/2014 00:00:00(Wed) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <0>;
			init_time,mday = <1>;
			init_time,mon = <0>;    /* +1 */
			init_time,year = <116>; /* 1900 + 114 */
			init_time,wday = <5>;

			s2mpw01-keys {
				button@1 {
					label = "pmic-keys: KEY_POWER";
					linux,code = <116>;
					wakeup = <1>;
				};
			};

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "BUCK1";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1300000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
					regulator-expected-consumer = <6>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "BUCK3";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				buck4_reg: BUCK4 {
					regulator-name = "BUCK4";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <2100000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <3>;
					regulator-ramp-delay = <12000>;
				};

				ldo1_reg: LDO1 {
					regulator-name = "vdd_ldo1";
					regulator-min-microvolt = <500000>;
					regulator-max-microvolt = <1350000>;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
					regulator-always-on;
					regulator-boot-on;
				};

				ldo2_reg: LDO2 {
					regulator-name = "vdd_ldo2";
					regulator-min-microvolt = <500000>;
					regulator-max-microvolt = <1350000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo3_reg: LDO3 {
					regulator-name = "vdd_ldo3";
					regulator-min-microvolt = <1500000>;
					regulator-max-microvolt = <2000000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <3>;
					regulator-ramp-delay = <12000>;
				};

				ldo4_reg: LDO4 {
					regulator-name = "vdd_ldo4";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1200000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <1>;
					regulator-ramp-delay = <12000>;
				};

				ldo5_reg: LDO5 {
					regulator-name = "vdd_ldo5";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1200000>;
					regulator-ramp-delay = <12000>;
				};

				ldo6_reg: LDO6 {
					regulator-name = "vdd_ldo6";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
					regulator-ramp-delay = <12000>;
					regulator-always-on;
				};

				ldo7_reg: LDO7 {
					regulator-name = "vdd_ldo7";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1200000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <3>;
					regulator-ramp-delay = <12000>;
				};

				ldo8_reg: LDO8 {
					regulator-name = "vdd_ldo8";
					regulator-min-microvolt = <1500000>;
					regulator-max-microvolt = <2000000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <0>;
					regulator-ramp-delay = <12000>;
				};

				ldo9_reg: LDO9 {
					regulator-name = "vdd_ldo9";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <0>;
					regulator-ramp-delay = <12000>;
				};

				ldo10_reg: LDO10 {
					regulator-name = "vdd_ldo10";
					regulator-min-microvolt = <1500000>;
					regulator-max-microvolt = <2000000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <1>;
					regulator-ramp-delay = <12000>;
				};
				/* Audio 1.8V */
				ldo11_reg: LDO11 {
					regulator-name = "vdd_aud_1.8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				//	regulator-always-on;
				//	regulator-boot-on;
					regulator-ramp-delay = <12000>;
				};
				/* Audio 3.1V */
				ldo12_reg: LDO12 {
					regulator-name = "vdd_aud_3.1";
					regulator-min-microvolt = <3100000>;
					regulator-max-microvolt = <3100000>;
				//	regulator-always-on;
				//	regulator-boot-on;
					regulator-ramp-delay = <12000>;
				};

				/* Touch */
				ldo13_reg: LDO13 {
					regulator-name = "vdd_tsp_1.8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
				};

				ldo14_reg: LDO14 {
					regulator-name = "vdd_ldo14";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-ramp-delay = <12000>;
				};

				ldo15_reg: LDO15 {
					regulator-name = "vdd_ldo15";
					regulator-min-microvolt = <1500000>;
					regulator-max-microvolt = <2000000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <1>;
					regulator-ramp-delay = <12000>;
				};

				/* Touch */
				ldo16_reg: LDO16 {
					regulator-name = "vdd_tsp_3.1";
					regulator-min-microvolt = <3100000>;
					regulator-max-microvolt = <3100000>;
					regulator-always-on;
					regulator-initial-mode = <3>;
					regulator-ramp-delay = <12000>;
				};

				/* LCD */
				ldo17_reg: LDO17 {
					regulator-name = "vdd_lcd_1.8";
					regulator-min-microvolt = <1500000>;
					regulator-max-microvolt = <2000000>;
					regulator-ramp-delay = <12000>;
				};

				/* OTS */
				ldo18_reg: LDO18 {
					regulator-name = "vdd_ots_3.1";
					regulator-min-microvolt = <3100000>;
					regulator-max-microvolt = <3100000>;
					regulator-ramp-delay = <12000>;
				};

				ldo19_reg: LDO19 {
					regulator-name = "vdd_ldo19";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-ramp-delay = <12000>;
				};

				ldo20_reg: LDO20 {
					regulator-name = "vdd_ldo20";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-ramp-delay = <12000>;
				};

				ldo21_reg: LDO21 {
					regulator-name = "vdd_ldo21";
					regulator-min-microvolt = <1500000>;
					regulator-max-microvolt = <2000000>;
					regulator-ramp-delay = <12000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <1>;
				};

				ldo22_reg: LDO22 {
					regulator-name = "vdd_ldo22";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1200000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <1>;
					regulator-ramp-delay = <12000>;
				};

				ldo23_reg: LDO23 {
					regulator-name = "vdd_ldo23";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-ramp-delay = <12000>;
				};
			};
		};
	};

