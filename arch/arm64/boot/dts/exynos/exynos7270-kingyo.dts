	i2c@13840000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;
		status = "okay";

		touchscreen@24 {
			compatible = "cy,cyttsp5_i2c_adapter";
			reg = <0x24>;
			interrupt-parent = <&gpa0>;
			interrupts = <2 0 0>;
			pinctrl-names = "default";
			pinctrl-0 = <&touch_irq &touch_rst>;
			cy,adapter_id = "cyttsp5_i2c_adapter";
			cy,core {
				cy,name = "cyttsp5_core";
				cy,irq_gpio = <&gpa0 2 0x1>;
				cy,rst_gpio = <&gpc3 2 0x1>;
				cy,hid_desc_register = <1>;
				cy,mt {
					cy,name = "cyttsp5_mt";
					cy,inp_dev_name = "cyttsp5_mt";
					cy,abs =
						/* ABS_MT_POSITION_X, CY_ABS_MIN_X, CY_ABS_MAX_X, 0, 0 */
						<0x35 0 390 0 0
						/* ABS_MT_POSITION_Y, CY_ABS_MIN_Y, CY_ABS_MAX_Y, 0, 0 */
						0x36 0 390 0 0
						/* ABS_MT_PRESSURE, CY_ABS_MIN_P, CY_ABS_MAX_P, 0, 0 */
						0x3a 0 255 0 0
						/* CY_IGNORE_VALUE, CY_ABS_MIN_W, CY_ABS_MAX_W, 0, 0 */
						0xffff 0 255 0 0
						/* ABS_MT_TRACKING_ID, CY_ABS_MIN_T, CY_ABS_MAX_T, 0, 0 */
						0x39 0 15 0 0
						/* ABS_MT_TOUCH_MAJOR, 0, 255, 0, 0 */
						0x30 0 255 0 0
						/* ABS_MT_TOUCH_MINOR, 0, 255, 0, 0 */
						0x31 0 255 0 0
						/* ABS_MT_ORIENTATION, -127, 127, 0, 0 */
						0x34 0xffffff81 127 0 0
						/* ABS_MT_TOOL_TYPE, 0, MT_TOOL_MAX, 0, 0 */
						0x37 0 1 0 0>;
						/* ABS_DISTANCE, 0, 255, 0, 0 */
						/*0x19 0 255 0 0>;*/
				};
			};
		};
	};

	spi@100C0000 {
		status = "okay";
		Msensors: MultiSensors@0 {
			compatible = "MultiSensors,24";
			spi-max-frequency = <20000000>;
			reg = <0x0>;
			nsw,sub_main_int = <&gpa0 3 0>;
			nsw,main_sub_int = <&gpf0 1 0>;
			pinctrl-names = "default";
			pinctrl-0 = <&sub_main_int &main_sub_int>;
			controller-data {
				samsung,spi-chip-select-mode = <1>;
				samsung,spi-feedback-delay = <0>;
			};
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&key_tool &key_app>;
		#address-cells = <1>;
		#size-cells = <0>;

		button@1 {
			label = "gpio-keys: VOLUME_UP";
			interrupts = <4 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <115>;
			gpios = <&gpa0 4 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: VOLUME_DOWN";
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <114>;
			gpios = <&gpa0 7 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	decon_fb@14830000 {
		/* HW TE */
		gpios = <&gpe0 2 0xf>;
		/* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		psr_mode = <2>;
		/* vclk_mif = 100MHz */
		mif-vclk = <100000000>;
		/* vclk = 12.5Mhz */
		disp-vclk = <12500000>;
		/* aclk = 208Mhz */
		disp-dvfs = <208000000>;
		status = "okay";
	};

	dsim_0: dsim@14800000 {
		lcd_info = <&auo_h120bln017>;
		/* lcd reset, lcd power */
		gpios = <&gpd2 0 0x1 &gpa0 1 0x1>;
		pinctrl-names = "default";
		pinctrl-0 = <&reset_oled &pwctl_oled>;
		regulator_ldo17 = "vdd_lcd_1.8";
		/* number of data lanes in use */
		data_lane_cnt = <1>;
		status = "okay";
	};

	auo_h120bln017: auo_h120bln017_custom {
		/* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		mode = <2>;
		/* custom (390 x 390) */
		panel_name = "auo_h120bln017";
		resolution = <390 390>;
		/* DISP I/F resolution width : 300, height : 507 */
		dispif_resol = <300 507>;
		size = <32 32>;
		timing,refresh = <45>;
		/* dsim H porch : hbp, hfp, hsa */
		timing,dsim_h-porch = <1 1 1>;
		/* dsim V porch : VBP, VFP, VSA */
		timing,dsim_v-porch = <10 2 1>;
		/* decon H porch : hbp, hfp, hsa */
		timing,decon_h-porch = <7 1 1>;
		/* decon V porch : VBP, VFP, VSA */
		timing,decon_v-porch = <10 2 1>;
		/* panel max speed : 384Mbps */
		timing,dsi-hs-clk = <384>;
		timing,pms = <2 118 2>;
		timing,dsi-escape-clk = <16>;
	};

	battery: sub_bat {
		compatible = "casio,sub-battery";
		status = "okay";
	};

	vibrator: sub_vib {
