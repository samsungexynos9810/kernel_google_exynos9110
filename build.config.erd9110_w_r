ARCH=arm64
CLANG_TRIPLE=aarch64-linux-gnu-
CROSS_COMPILE=aarch64-linux-androidkernel-

CC=clang
LD=ld.lld
CLANG_PREBUILT_BIN=prebuilts-master/clang/host/linux-x86/clang-r383902/bin
SET_TOOLCHAIN=1

DEFCONFIG=erd9110_defconfig
TARGET_SOC=exynos9110

TARGET_DTBO_CFG=exynos9110_dtboimg.cfg
SEPERATE_KERNEL_OBJ=true

FILES="
arch/arm64/boot/Image:kernel
arch/arm64/boot/dts/exynos/exynos9110.dtb:dtb.img
dtbo.img:dtbo.img
vmlinux:vmlinux
System.map:System.map
"
